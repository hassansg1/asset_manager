+        $request->validate($this->model->rules);
+        $this->model->saveFormData($this->model, $request);
+
+        flashSuccess(getLang($this->heading . " Successfully Created."));
+
+        if (isset($request->add_new)) return redirect(route($this->route . ".create"));
